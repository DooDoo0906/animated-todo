{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "allowArbitraryExtensions": true,
    "jsx": "react-native",
    "lib": [
      "DOM",
      "ESNext"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
  }
}
